{
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-import": "^1.6.2",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.7",
    "electron": "^1.6.11",
    "style-loader": "^0.19.0",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.4"
  },
  "scripts": {
    "start": "electron .",
    "webpack": "webpack --progress --watch",
    "package": "electron-packager ./ test --win --out test --arch=x64 --version 1.7.9 --overwrite --ignore=node_modules --version-string.CompanyName=test --version-string.ProductName=test"
  },
  "build": {
    "productName": "PathInsight-local",
    "appId": "org.develar.ElectronReact",
    "files": [
      "dist/",
      "node_modules/",
      "app.html",
      "main.prod.js",
      "main.prod.js.map",
      "package.json"
    ],
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "win": {
      "target": [
        "nsis"
      ]
    },
    "linux": {
      "target": [
        "deb",
        "AppImage"
      ]
    },
    "directories": {
      "buildResources": "resources",
      "output": "release"
    }
  },
  "name": "dpas_electron",
  "version": "0.0.1",
  "main": "main.js",
  "author": "WenqiXie <szqq602@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "antd": "^2.13.9",
    "express": "^4.16.2",
    "express-http-proxy": "^1.0.7",
    "mongoid-js": "^1.1.3",
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  }
}
